   "source": [
    "import numpy as np\n",
    "X=np.eye(4)\n",
    "Y=np.ones((4,6))\n",
    "Z=np.eye(6)\n",
    "ao=1\n",
    "ax=1.2\n",
    "ay=2.94\n",
    "az=-2.1\n",
    "A=np.array([[ao],[ax],[ay],[az]])\n",
    "B=X.T.dot(A)\n",
    "C=Y.T.dot(B)\n",
    "D=Z.T.dot(C)\n",
    "\n",
    "print(int(D[2][0]))\n",
    "print(A,X,B,Y,C,Z,D,sep='\\n\\n')"
   ]
